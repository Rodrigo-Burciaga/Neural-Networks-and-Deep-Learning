= tf.Variable(tf.random_normal([1]), name = \"W\") \n","b = tf.Variable(tf.random_normal([1]), name = \"b\") \n","# Hypothesis \n","y_pred = tf.add(tf.multiply(X, W), b) \n","\n","# Mean Squared Error Cost Function \n","cost = tf.reduce_sum(tf.pow(y_pred - Y, 2)) / (2 * n) \n","scalar_summary = tf.summary.scalar('cost', cost)\n","\n","# Gradient Descent Optimizer \n","optimizer = tf.train.GradientDescentOptimizer(learning_rate).minimize(cost) \n","\n","# Global Variables Initializer \n","init = tf.global_variables_initializer() \n"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"gDjevfsZZpDP","colab_type":"code","outputId":"ea035d5f-df56-4511-c89e-b0cb978cbbf5","executionInfo":{"status":"ok","timestamp":1561616996895,"user_tz":300,"elapsed":55025,"user":{"displayName":"Rodrigo Andr√©s Burciaga Ornelas","photoUrl":"","userId":"01811326954830511276"}},"colab":{"base_uri":"https://localhost:8080/","height":384}},"source":["# Starting the Tensorflow Session \n","with tf.Session() as sess:\n","\n","  # Initializing the Variables \n","  sess.run(init) \n","  writer = tf.summary.FileWriter('./graphs', sess.graph)\n","\n","  # Iterating through all the epochs \n","  for epoch in range(training_epochs): \n","\n","    # Feeding each data point into the optimizer using Feed Dictionary \n","    for (_x, _y) in zip(x, y):\n","      sess.run(optimizer, feed_dict = {X : _x, Y : _y})\n","\n","    # Displaying the result after every 50 epochs \n","    if (epoch + 1) % 50 == 0:\n","      # Calculating the cost a every epoch \n","      c, summ = sess.run([cost,scalar_summary], feed_dict = {X : x, Y : y})\n","      writer.add_summary(summ, epoch + 1)\n","      \n","      print(\"Epoch\", (epoch + 1), \": cost =\", c, \"W =\", sess.run(W), \n","            \"b =\", sess.run(b)) \n","    x, y = shuffle(x, y, random_state=0)\n","    \n","  writer.flush()\n","  # Storing necessary values to be used outside the Session \n","  training_cost = sess.run(cost, feed_dict ={X: x, Y: y}) \n","  weight = sess.run(W) \n","  bias = sess.run(b) \n"],"execution_count":0,"outputs":[{"output_type":"stream","text":["Epoch 50 : cost = 4.556802 W = [0.9764579] b = [1.3841699]\n","Epoch 100 : cost = 4.987322 W = [0.9453811] b = [1.3947288]\n","Epoch 150 : cost = 4.6009817 W = [0.96700686] b = [1.4057727]\n","Epoch 200 : cost = 4.8537445 W = [1.0045137] b = [1.4158075]\n","Epoch 250 : cost = 5.420532 W = [0.93099374] b = [1.4223018]\n","Epoch 300 : cost = 4.5545354 W = [0.9777849] b = [1.430827]\n","Epoch 350 : cost = 4.7159324 W = [0.9967192] b = [1.4361161]\n","Epoch 400 : cost = 4.7308106 W = [0.99744296] b = [1.4420793]\n","Epoch 450 : cost = 4.554934 W = [0.9780005] b = [1.4457288]\n","Epoch 500 : cost = 5.3223057 W = [0.9328818] b = [1.44875]\n","Epoch 550 : cost = 4.5621295 W = [0.971718] b = [1.4537812]\n","Epoch 600 : cost = 4.6301227 W = [0.9624403] b = [1.4564878]\n","Epoch 650 : cost = 4.5545974 W = [0.97458076] b = [1.4614327]\n","Epoch 700 : cost = 4.9955873 W = [0.94298273] b = [1.4624567]\n","Epoch 750 : cost = 4.602202 W = [0.9649614] b = [1.4651617]\n","Epoch 800 : cost = 4.8584967 W = [1.0031971] b = [1.4678922]\n","Epoch 850 : cost = 5.4263134 W = [0.9294471] b = [1.4679567]\n","Epoch 900 : cost = 4.5540156 W = [0.9766019] b = [1.4708672]\n","Epoch 950 : cost = 4.715628 W = [0.9956707] b = [1.4712214]\n","Epoch 1000 : cost = 4.729349 W = [0.9964509] b = [1.4728613]\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"WlrpmfDYXC4p","colab_type":"code","outputId":"269bad1e-5e29-4551-a2cd-b2c8fb495270","executionInfo":{"status":"ok","timestamp":1561617027911,"user_tz":300,"elapsed":704,"user":{"displayName":"Rodrigo Andr√©s Burciaga Ornelas","photoUrl":"","userId":"01811326954830511276"}},"colab":{"base_uri":"https://localhost:8080/","height":54}},"source":["# Calculating the predictions \n","predictions = weight * x + bias \n","print(\"Training cost =\", training_cost, \"Weight =\", weight, \"bias =\", bias, '\\n') \n"]